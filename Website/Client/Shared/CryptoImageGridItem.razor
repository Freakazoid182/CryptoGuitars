<MudItem>
    <MudPaper Height="300px" Width="300px" Elevation="1">
        <img class="ma-5" src="sample-data/crypto-images/@ImageName" width="200" height="200">
        <MudText Class=ml-3>Image Title</MudText>
        <MudText Class=ml-3>Created on: @DateTime.Now.ToShortDateString()</MudText>
    </MudPaper>
</MudItem>

@code{

    [Parameter]
    public string? ImageName { get; set; }
}